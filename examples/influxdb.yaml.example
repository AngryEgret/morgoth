---
engine:
  influxdb:
    host: 192.168.1.1
    port: 8086
    user: root
    password: root
    database: morgoth

metrics:
  - pattern: .*
    detectors:
      - kstest: {}
    notifiers:
    schedule:
      rotations:
        - {period: 1m, resolution: 1s}
      delay: 65s

fittings:
  - rest:
      port: 7000
  - graphite: {}

logging:
    level: INFO
